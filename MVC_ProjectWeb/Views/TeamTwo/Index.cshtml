
@{ 

    var teams = ViewBag.Listado;

}


<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Description</th>
            <th>Country</th>
            <th>Titles</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in teams)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Description</td>
            <td>@item.Country</td>
            <td>@item.Titles</td>
        </tr>
        }


    </tbody>
    
</table>